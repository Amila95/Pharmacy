
            <!-- /.box-header -->
            <div class="row">
                <div class="col-xs-12">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="box-title">Reordering Level Table</h3>

                            <div class="box-tools">
                                <div class="input-group input-group-sm" style="width: 150px;">
                                    <input type="text" name="table_search" class="form-control pull-right" placeholder="Search">

                                    <div class="input-group-btn">
                                        <button type="submit" class="btn btn-default"><i class="fa fa-search"></i></button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body table-responsive no-padding">
                            <table class="table table-hover">
                                <tr>
                                    <th>logo</th>
                                    <th>Product Name</th>
                                    <th>Batch NO</th>
                                    <th>Current Stoke</th>
                                    <th>Ex Date</th>
                                    <!-- <th>Sent Date</th>-->
                                    <th></th>
                                </tr>
                                {{#each member}}
                                    <tr>

                                        <td><img src={{this.Image}} class="img-circle" width="50" height="50"></td>
                                        <td>{{this.product_name}}</td>
                                        <td>{{this.batch_No}}</td>
                                        <td>{{this.qty}}</td>
                                        <td>{{this.ex_date}}</td>
                                        <td><a href='/admin/message{{this.dis_id}}'><span class="label label-success">View</span></a></td>



                                    </tr>
                                {{/each}}
                            </table>
                        </div>
                        <!-- /.box-body -->
                    </div>
                    <!-- /.box -->
                </div>
            </div>
