<section class="content-header">
      <h1>
        Company Profile
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Examples</a></li>
        <li class="active">User profile</li>
      </ol>
    </section>
      {{#each companies}}
      <div class="row">
        <div class="col-md-3">

          <!-- Profile Image -->
          <div class="box box-primary">
            <div class="box-body box-profile">
              <img class="profile-user-img img-responsive img-circle" src= {{this.company_logo}} alt="User profile picture">

              <h3 class="profile-username text-center">{{this.company_name}}</h3>

              <p class="text-muted text-center">Medicine Supper</p>

              <ul class="list-group list-group-unbordered">
              <li class="list-group-item">
                  <b>Company ID</b> <a class="pull-right">{{company_id}}</a>
                </li>
                <li class="list-group-item">
                  <b>Email</b> <a class="pull-right">{{company_email}}</a>
                </li>
                <li class="list-group-item">
                  <b>Contact No</b> <a class="pull-right">{{company_contact_no}}</a>
                </li>
                
              </ul>

              <a href="#" class="btn btn-primary btn-block"><b>Follow</b></a>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->

          <!-- About Me Box -->
          <div class="box box-primary">
            <div class="box-header with-border">
              <h3 class="box-title">About </h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <strong><i class="fa fa-book margin-r-5"></i> Discription</strong>

              <p class="text-muted">
                {{company_discription}}
              </p>

              <hr>

              <strong><i class="fa fa-map-marker margin-r-5"></i> Address</strong>

              <p class="text-muted">{{company_address}}</p>

              <hr>

              
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->      
      
      {{/each}}
<div class="col-md-9">
          <div class="nav-tabs-custom">
            <ul class="nav nav-tabs">
              <li class="active"><a href="#activity" data-toggle="tab">A-I</a></li>
              <li><a href="#timeline" data-toggle="tab">J-P</a></li>
              <li><a href="#settings" data-toggle="tab">R-Z</a></li>
            </ul>
            <div class="tab-content">
              <div class="active tab-pane" id="activity">
                <!-- Post -->
                {{#each ae}}
                <div class="post">
                  <div class="user-block">
                    <img class="img-circle img-bordered-sm" src={{this.Image}} alt="user image">
                        <span class="username">
                          <a href="/admin/view_product_profile{{this.product_id}}">{{this.product_name}}</a>
                          <a href="/admin/view_product_profile{{this.product_id}}" class="pull-right btn-box-tool"><i class="fa fa-times"></i></a>
                        </span>
                    
                  </div>
                  
                </div>
                <!-- /.post -->
                {{/each}}

                

                <!-- Post -->
                
              </div>
              <!-- /.tab-pane -->
              <div class="tab-pane" id="timeline">
                <!-- Post -->
                {{#each jq}}
                <div class="post">
                  <div class="user-block">
                    <img class="img-circle img-bordered-sm" src={{this.Image}} alt="user image">
                        <span class="username">
                          <a href="/admin/view_product_profile{{this.product_id}}">{{this.product_name}}</a>
                          <a href="/admin/view_product_profile{{this.product_id}}" class="pull-right btn-box-tool"><i class="fa fa-times"></i></a>
                        </span>
                    
                  </div>
                  
                </div>
                <!-- /.post -->
                {{/each}}
              </div>

              <div class="tab-pane" id="settings">
                <!-- Post -->
                {{#each qz}}
                <div class="post">
                  <div class="user-block">
                    <img class="img-circle img-bordered-sm" src={{this.Image}} alt="user image">
                        <span class="username">
                          <a href="/admin/view_product_profile{{this.product_id}}">{{this.product_name}}</a>
                          <a href="/admin/view_product_profile{{this.product_id}}" class="pull-right btn-box-tool"><i class="fa fa-times"></i></a>
                        </span>
                    
                  </div>
                  
                </div>
                <!-- /.post -->
                {{/each}}
              </div>
              <!-- /.tab-pane -->
            </div>
            <!-- /.tab-content -->
          </div>
          <!-- /.nav-tabs-custom -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->

    </section>
    <!-- /.content -->
  </div>
</div>